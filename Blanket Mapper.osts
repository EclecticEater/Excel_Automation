{"version":"0.2.0","body":"function main(workbook: ExcelScript.Workbook) {\n\n  var sheet = workbook.getActiveWorksheet();\n\n  const column = \"Malta\"\n\n  let dataRange = sheet.getUsedRange();\n\n  let headerRow = dataRange.getRow(0).getValues()[0];\n\n  let wordCol = headerRow.indexOf(column);\n\n  if (wordCol !== -1) {\n\n    let columnRange = sheet.getRangeByIndexes(1, wordCol, dataRange.getRowCount() - 1, 1);\n\n    let values = columnRange.getValues();\n\n    values.forEach((rowItem, rowIndex) => {\n\n      values[rowIndex].forEach((columnItem, ColumnIndex) => {\n\n        let cells = columnItem.toString();\n\n        columnRange.getCell(rowIndex, ColumnIndex).setValue(cells + \"\\n\" + \"ID\");\n\n        columnRange.getCell(rowIndex, ColumnIndex + 1).setValue(cells + \"\\n\" + \"Credit\");\n\n        columnRange.getCell(rowIndex, ColumnIndex + 2).setValue(cells + \"\\n\" + \"Justification\");\n\n\n      });\n\n    });\n  }\n} ","description":"","parameterInfo":"{\"originalParameterOrder\":[],\"parameterSchema\":{\"type\":\"object\",\"default\":{},\"x-ms-visibility\":\"internal\"},\"returnSchema\":{\"type\":\"object\",\"properties\":{}}}","apiInfo":"{\"variant\":\"synchronous\",\"variantVersion\":2}"}